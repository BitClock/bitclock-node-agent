{
  "instrument": false,
  "sourceMap": false,
  "lines": 90,
  "statements": 90,
  "functions": 90,
  "branches": 90,
  "reporter": [
    "text",
    "text-summary",
    "html"
  ],
  "cache": true,
  "all": true,
  "include": [
    "lib/**/*.js"
  ],
  "check-coverage": true,
  "report-dir": "./coverage"
}
